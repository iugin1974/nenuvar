  \livretAct\line { ACTE V }
  \livretDescAtt\wordwrap-center\italic {
    Le Theatre change, & represente une Campagne "agreable ;"
    la nuit se dissipe insensiblement,
    & cede au jour qui commence à "paroître ;"
    Phaëton assis sur le Char du Soleil,
    s'éleve sur l'horizon.
  }
  \livretScene\line { SCENE PREMIERE }
  \livretDescAtt\wordwrap-center { 
    Climene, & un des deux Rois tributaires de Merops.
  }
  \livretRef #'FAAclymene
  \livretPers CLIMENE
  \livretVerC { Assembles-vous Habitans de ces lieux. }
  \livretVerC { Le sommeil qui ferme vos yeux }
  \livretVer { Vous retient trop long-tems dans une paix profonde : }
  \livretVerC { Mon Fils fait voir qu'il est du sang des Dieux. }
  \livretVer { Sur le Char de son Pere il brille dans les Cieux, }
  \livretVerC { Que votre zele me seconde, }
  \livretVer { Celevrez avec moi son destin glorieux. }
  \livretVerC { Que l'on chante, que tout réponde, }
  \livretVerC { C'est un Soleil nouveau }
  \livretVerC { Qui donne la lumiere au Monde, }
  \livretVerC { C'est un Soleil nouveau }
  \livretVerC { Qui donne un jour si beau. }
  \livretPers\line { CLIMENE, & LE ROI TRIBUTAIRE DE MEROPS }
  \livretVerC { C'est un Soleil nouveau }
  \livretVerC { Qui donne la lumiere au Monde, }
  \livretVerC { C'est un Soleil nouveau }
  \livretVerC { Qui donne un jour si beau. }
  \livretDesc\justify\italic {
    Climene transportée de joie, court de tous côtez publier
    la gloire de son Fils, les Peuples d'Egypte qui entendent
    sa voix, s'empressent de la suivre.
  }

  \livretScene\line { SCENE II }
  \livretDescAtt\wordwrap-center {
    Epaphus, troupe de Peuples qui suivent Climene.
  }
  \livretRef #'FBAepaphus
  \livretPers EPAPHUS
  \livretVer { Dieu qui vous déclarez mon Pere, }
  \livretVer { Maître des Dieux, c'est en vous que j'espere ; }
  \livretVer { M'abandonnerez-vous au desespoir fatal }
  \livretVerC { De voir triompher mon Rival ? }
  \livretVerC { On suit les transports de sa Mere ; }
  \livretVerC { On me méprise, on le revere ; }
  \livretVer { Tout sert à son bonheur, tout irrite mon mal. }
  \livretVerC { Il obtient ce qui m'a sçu plaire, }
  \livretVerC { Il monte au Ciel, il nous éclaire, }
  \livretVer { Il me voit accablé d'un tourment sans égal. }
  \livretVer { Dieu qui vous déclarez mon Pere, }
  \livretVer { Maître des Dieux, c'est en vous que j'espere ; }
  \livretVer { M'abandonnerez-vous au desespoir fatal }
  \livretVerC { De voir triompher mon Rival ? }

  \livretScene\line { SCENE III }
  \livretDescAtt\wordwrap-center { Epaphus, Libye }
  \livretRef #'FCAlibyeEpaphus
  \livretPers LIBYE
  \livretVerC { O rigoureux martire }
  \livretVer { De n'oser découvrir de mortelles douleurs ! }
  \livretVer { Mon destin paroît beau, tout le monde l'admire, }
  \livretVerC { Cependant, je soupire, }
  \livretVerC { Je pleure mes malheurs ; }
  \livretVer { Du severe devoir le tirannique empire }
  \livretVer { Me contraint à cacher mes soupirs & mes pleurs. }
  \livretVerC { O rigoureux martire }
  \livretVer { De n'oser découvrir de mortelles douleurs ! }
  \livretVerC { Dieux ! Epaphus ! }
  \livretPers EPAPHUS
  \livretVerC { \transparent { Dieux ! Epaphus ! } Belle Princesse... }
  \livretPers LIBYE
  \livretVerC { N'augmentez pas le desordre où je suis. }
  \livretPers EPAPHUS
  \livretVerC { Vous me fuiez ? }
  \livretPers LIBYE
  \livretVerC { \transparent { Vous me fuiez ? } Quelle foiblesse ! }
  \livretVerC { Je le devrois ; mais je ne puis. }
  \livretVer { Helas ! en nous voiant, nous redoublons nos peines. }
  \livretPers EPAPHUS
  \livretVerC { Que dans mes maux il m'est doux de vous voir ! }
  \livretPers LIBYE
  \livretVer { Je suis à Phaëton par des loix souveraines. }
}
\markup\n-columns {
  \livretPers EPAPHUS
  \livretVerC { Vous n'êtes pas encore en son pouvoir. }
  \livretVer { Mon Pere est Souverain du Ciel & de la Terre, }
  \livretVer { Esperons au secours qu'il peut nous reserver. }
  \livretVerC { Plus mon Rival s'empresse à s'élever }
  \livretVerC { Plus son orgueil l'approche du Tonnerre. }
  \livretPers LIBYE
  \livretVer { Je n'ose plus songer qu'à suivre mon devoir, }
  \livretVerC { L'esperance nous est ravie. }
  \livretPers EPAPHUS
  \livretVerC { Ah ! si vous m'ôtez tout espoir, }
  \livretVerC { Vous m'ôterez la vie. }
  \livretVer { J'ose attendre du Sort quelque heureux changement, }
  \livretVer { L'Amour doit esperer jusqu'au dernier moment. }
  \livretPers LIBYE
  \livretVerC { Notre disgrace est certaine, }
  \livretVerC { Vous esperez vainement. }
  \livretPers EPAPHUS
  \livretVerC { L'esperance la plus vaine }
  \livretVerC { Flatte un malheureux Amant. }
  \livretVerC { Helas ! une chaîne si belle }
  \livretVerC { Devoit être éternelle ! }
  \livretVerC { Helas ! de si tendres amours }
  \livretVerC { Devoient durer toujours ! }

