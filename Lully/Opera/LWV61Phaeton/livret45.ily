  \livretRef #'FEBchoeur
  \livretPers\line { LE CHŒUR }
  \livretVerC { Dieux ! quel feu vient par tout s'étendre ! }
  \livretVerC { Dieux ! tout va se réduire en cendre ! }
  \livretVerC { Quelle ardeur penetre en tous lieux ! }
  \livretVerC { Où fuirons-nous ! ô justes Dieux ! }

  \livretScene\line { SCENE VI }
  \livretDescAtt\justify {
    La Déesse de la Terre, Theone, Merops, Climene,
    les deux Rois tributaires de "Merops :"
    troupes de divers "Peuples :"
    troupes de Pasteurs Egyptiens, & de Bergeres Egyptiennes.
  }
  \livretRef #'FFAdeesse
  \livretPers\line { La Déesse de la Terre }
  \livretVer { C'est votre secours que j'implore, }
  \livretVer { Jupiter, sauvés-moi du feu qui me devore. }
  \livretVer { Ai-je pû meriter un si cruel tourment ? }
  \livretVerC { Ah ! s'il faut qu'un embrasement }
  \livretVerC { A la fin me reduise en poudre, }
  \livretVer { Que je ne brûle au moins que du feu de la foudre : }
  \livretVerC { Grand Dieu, ne me refusez pas }
  \livretVer { La gloire de perir d'un coup de votre bras. }
  \livretVer { Roi des dieux, armés-vous, il n'est plus temps d'attendre, }
  \livretVerC { Tout l'Empire qui suit vos loix }
  \livretVer { Bien-tôt ne sera plus qu'un vain monceau de cendre. }
  \livretVer { Les Fleuves vont tarir ; les Villes, & les Bois, }
  \livretVer { Les Monts les plus glacez, tout s'embrase à la fois, }
  \livretVerC { Les Cieux ne peuvent s'en défendre... }
  \livretVerC { Ah ! je sens suffoquer ma voix }
  \livretVerC { Avec peine je respire, }
  \livretVerC { Au milieu de tant de feux. }
  \livretVerC { Il faut que je me retire }
  \livretVerC { Dans mes Antres les plus creux. }

  \livretScene\line { SCENE VII }
  \livretDescAtt\column {
    \justify {
      Phaëton, Merops, Climene, Libye, Theone
      les deux Rois tributaires de "Merops :"
      troupes de divers "Peuples :"
      troupes de Pasteurs Egyptiens, & de Bergeres Egyptiennes.
    }
    \justify\italic {
      Phaëton paroît en desordre sur le Char du Soleil,
      qu'il ne peut plus conduire.
    }
  }
  \livretRef #'FGAchoeur
  \livretPers\line { LE CHŒUR }
  \livretVer { O Dieu qui lancez le Tonnerre, }
  \livretVer { Hâtés-vous de sauver la Terre : }
  \livretVer { Nous brûlons, nous allons perir ; }
  \livretVer { Venez, ô ! Jupiter, venez nous secourir. }

  \livretScene\line { SCENE DERNIERE }
  \livretDescAtt\justify {
    Jupiter, Phaëton, Merops, Climene, Libye, Theone
    les deux Rois tributaires de "Merops :"
