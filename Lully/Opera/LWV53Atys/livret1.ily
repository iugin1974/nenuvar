\appendixSubSection "Acte I"
\markuplist \column-lines {
  \livretAct \line { ACTE PREMIER. }
  \livretDesc \wordwrap-center {
    Le Theatre represente une montagne
    consacrée à Cybele.
  }
  \livretScene \line { SCENE PREMIERE. }
  \livretRef #'BAAatys
  \livretPers \line { ATYS. }
  \livretVerC { Allons, allons, accourez tous, }
  \livretVerC { Cybele va descendre. }
  \livretVer { Trop heureux Phrygiens, venez icy l'attendre. }
  \livretVerC { Mille Peuples seront jaloux }
  \livretVerC { Des faveurs que sur nous }
  \livretVerC { Sa bonté va répandre. }
  %%%
  \livretScene \line { SCENE SECONDE. }
  \livretRef #'BBAatysIdas
  \livretPers \line { IDAS, ATYS. }
  \livretVerC { Allons, allons, accourez tous, }
  \livretVerC { Cybele va descendre. }

  \livretPers ATYS.
  \livretVer { Le Soleil peint nos champs des plus vives couleurs, }
  \livretVerC { Il a seché les pleurs }
  \livretVer { Que sur l'émail des prez a répandu l'Aurore ; }
  \livretVer { Et ses rayons nouveaux ont déja fait éclorre }
  \livretVerC { Mille nouvelles fleurs. }

  \livretPers IDAS.
  \livretVerC { Vous veillez lorsque tout sommeille ; }
  \livretVerC { Vous nous éveillez si matin }
  \livretVerC { Que vous ferez croire à la fin }
  \livretVerC { Que c'est l'Amour qui vous éveille. }

  \livretPers ATYS.
  \livretVer { Non tu dois mieux juger du party que je prens. }
  \livretVer { Mon cœur veut fuir toûjours les soins et les misteres ; }
  \livretVer { J'ayme l'heureuse paix des cœurs indifferents ; }
  \livretVerC { Si leurs plaisirs ne sont pas grands, }
  \livretVerC { Au moins leurs peines sont legeres. }

  \livretPers IDAS.
  \livretVerC { Tost ou tard l'Amour est vainqueur, }
  \livretVerC { En vain les plus fiers s'en deffendent, }
  \livretVerC { On ne peut refuser son cœur }
  \livretVerC { A de beaux yeux qui le demandent. }
  \livretVer { Atys, ne feignez plus, je sçais votre secret. }
  \livretVerC { Ne craignez rien, je suis discret. }
  \livretVerC { Dans un bois solitaire, et sombre, }
  \livretVer { L'indifferent Atys se croyoit seul, un jour ; }
  \livretVer { Sous un feüillage épais où je resvois à l'ombre, }
  \livretVerC { Je l'entendis parler d'amour. }

  \livretPers ATYS.
  \livretVer { Si je parle d'amour, c'est contre son empire, }
  \livretVerC { J'en fais mon plus doux entretien. }

  \livretPers IDAS.
  \livretVerC { Tel se vante de n'aimer rien, }
  \livretVerC { Dont le cœur en secret soûpire. }
  \livretVer { J'entendis vos regrets, et je les sçais si bien }
  \livretVer { Que si vous en doutez je vais vous les redire. }
  \null
  \livretVer { Amans qui vous plaignez, vous estes trop heureux : }
  \livretVer { Mon cœur de tous les cœurs est le plus amoureux, }
  \livretVer { Et tout prés d'expirer je suis reduit à feindre ; }
  \livretVerC { Que c'est un tourment rigoureux }
  \livretVerC { De mourir d'amour sans se plaindre ! }
  \livretVer { Amans qui vous plaignez, vous estes trop heureux. }

  \livretPers ATYS.
  \livretVer { Idas, il est trop vray, mon cœur n'est que trop tendre, }
  \livretVer { L'Amour me fait sentir ses plus funestes coups. }
  \livretVer { Qu'aucun autre que toy n'en puisse rien apprendre. }

  %%%
  \livretScene \line { SCENE TROISIEME. }
  \livretDescAtt \wordwrap-center { SANGARIDE, DORIS, ATYS, IDAS. }
  \livretRef #'BCAsdai
  \livretPers \line { SANGARIDE, & DORIS. }
  \livretVerC { Allons, allons, accourez tous, }
  \livretVerC { Cybele va descendre. }

  \livretPers SANGARIDE.
  \livretVerC { Que dans nos concerts les plus doux, }
  \livretVerC { Son nom sacré se fasse entendre. }

  \livretPers ATYS.
  \livretVer { Sur l'Univers entier son pouvoir doit s'étendre. }

  \livretPers SANGARIDE.
  \livretVer { Les Dieux suivent ses loix et craignent son couroux. }

  \livretPers \line { ATYS, SANGARIDE, IDAS, DORIS. }
  \livretVer { Quels honneurs ! quels respects ne doit-on point luy rendre ? }
  \livretVerC { Allons, allons, accourez tous, }
  \livretVerC { Cybele va descendre. }

  \livretPers SANGARIDE.
  \livretVer { Escoutons les oyseaux de ces bois d'alentour, }
  \livretVer { Ils remplissent leurs chants d'une douceur nouvelle. }
  \livretVerC { On diroit que dans ce beau jour, }
  \livretVerC { Ils ne parlent que de Cybele. }

  \livretPers ATYS.
  \livretVer { Si vous les écoutez, ils parleront d'amour. }
  \livretVerC { Un Roy redoutable, }
  \livretVerC { Amoureux, aimable, }
  \livretVerC { Va devenir vostre espoux ; }
  \livretVerC { Tout parle d'amour pour vous. }

  \livretPers SANGARIDE.
  \livretVer { Il est vray, je triomphe, et j'aime ma victoire. }
  \livretVer { Quand l'Amour fait regner, est-il un plus grand bien ? }
  \livretVerC { Pour vous, Atys, vous n'aimez rien, }
  \livretVerC { Et vous en faites gloire. }

  \livretPers ATYS.
  \livretVerC { L'Amour fait trop verser de pleurs ; }
  \livretVerC { Souvent ses douceurs sont mortelles. }
  \livretVerC { Il ne faut regarder les Belles }
  \livretVerC { Que comme on voit d'aimables fleurs. }
  \livretVerC { J'aime les Roses nouvelles, }
  \livretVerC { J'aime les voir s'embellir, }
  \livretVerC { Sans leurs épines cruelles, }
  \livretVerC { J'aimerois à les cüeillir. }

  \livretPers SANGARIDE.
  \livretVerC { Quand le peril est agreable, }
  \livretVerC { Le moyen de s'en allarmer ? }
  \livretVerC { Est-ce un grand mal de trop aimer }
  \livretVerC { Ce que l'on trouve aimable ? }
  \livretVer { Peut-on estre insensible aux plus charmans appas ? }

  \livretPers ATYS.
  \livretVerC { Non vous ne me connoissez pas. }
  \livretVer { Je me deffens d'aimer autant qu'il m'est possible ; }
  \livretVerC { Si j'aimois, un jour, par malheur, }
  \livretVerC { Je connoy bien mon cœur }
  \livretVerC { Il seroit trop sensible. }
  \livretVer { Mais il faut que chacun s'assemble prés de vous, }
  \livretVerC { Cybele pourroit nous surprendre. }

  \livretPers \line { IDAS, ATYS. }
  \livretVerC { Allons, allons, accourez tous, }
  \livretVerC { Cybele va descendre. }
  %%%
  \livretScene \line { SCENE QUATRIEME. }
  \livretDescAtt \wordwrap-center { SANGARIDE, DORIS. }
  \livretRef #'BDAsangarideDoris
  \livretPers \line { SANGARIDE. }
  \livretVerC { Atys est trop heureux. }

  \livretPers DORIS.
  \livretVer { L'amitié fut toûjours égale entre vous deux, }
  \livretVerC { Et le sang d'assez prés vous lie : }
  \livretVer { Quel que soit son bon-heur, luy portez-vous envie ? }
  \livretVer { Vous, qu'aujourd'huy l'Hymen avec de si beaux nœuds }
  \livretVerC { Doit unir au Roy de Phrygie ? }

  \livretPers SANGARIDE.
  \livretVerC { Atys, est trop heureux. }
  \livretVer { Souverain de son cœur, maistre de tous ses vœux, }
  \livretVerC { Sans crainte, sans melancolie, }
  \livretVer { Il joüit en repos des beaux jours de sa vie ; }
  \livretVer { Atys ne connoît point les tourmens amoureux, }
  \livretVerC { Atys est trop heureux. }

  \livretPers DORIS.
  \livretVer { Quel mal vous fait l'Amour ? vostre chagrin m'estonne. }

  \livretPers SANGARIDE.
  \livretVer { Je te fie un secret qui n'est sceu de personne. }
  \livretVerC { Je devrois aimer un Amant }
  \livretVerC { Qui m'offre une Couronne ; }
  \livretVerC { Mais, helas ! vainement }
  \livretVerC { Le Devoir me l'ordonne, }
  \livretVerC { L'Amour, pour mon tourment, }
  \livretVerC { En ordonne autrement. }

  \livretPers DORIS.
  \livretVer { Aimeriez-vous Atys, luy dont l'indifference }
  \livretVer { Brave avec tant d'orgüeil l'Amour et sa puissance ? }

  \livretPers SANGARIDE.
  \livretVer { J'aime, Atys, en secret, mon crime, est sans témoins. }
  \livretVer { Pour vaincre mon amour, je mets tout en usage, }
  \livretVer { J'appelle ma raison, j'anime mon courage ; }
  \livretVerC { Mais à quoy servent tous mes soins ? }
  \livretVerC { Mon cœur en souffre davantage, }
  \livretVerC { Et n'en aime pas moins. }

  \livretPers DORIS.
  \livretVerC { C'est le commun deffaut des Belles. }
  \livretVerC { L'ardeur des conquestes nouvelles }
  \livretVer { Fait negliger les cœurs qu'on a trop tost charmez, }
  \livretVer { Et les Indifferents sont quelquefois aimez }
  \livretVerC { Aux dépens des Amants fidelles. }
  \livretVer { Mais vous vous esposez à des peines cruelles. }

  \livretPers SANGARIDE.
  \livretVer { Toûjours aux yeux d'Atys je seray sans appas ; }
  \livretVer { Je le sçay, j'y consens, je veux, s'il est possible, }
  \livretVerC { Qu'il soit encor plus insensible ; }
  \livretVer { S'il me pouvoit aimer, que deviendrois-je ? helas ! }
  \livretVer { C'est mon plus grand bon-heur qu'Atys ne m'aime pas. }
  \livretVer { Je pretens estre heureuse, au moins, en apparence ; }
  \livretVer { Au destin d'un grand Roy je me vais attacher. }

  \livretPers \line { SANGARIDE, & DORIS. }
  \livretVer { Un amour malheureux dont le devoir s'offence, }
  \livretVerC { Se doit condamner au silence ; }
  \livretVer { Un amour malheureux qu'on nous peut reprocher, }
  \livretVerC { Ne sçauroit trop bien se cacher. }
  %%%
  \livretScene \line { SCENE CINQUIESME. }
  \livretDescAtt \wordwrap-center { ATYS, SANGARIDE, DORIS. }
  \livretRef #'BEAatysDoris
  \livretPers ATYS.
  \livretVerC { On voit dans ces campagnes }
  \livretVerC { Tous nos Phrygiens s'avancer. }

  \livretPers DORIS.
  \livretVerC { Je vais prendre soin de presser }
  \livretVerC { Les Nymphes nos Compagnes. }
  %%%
  \livretScene \line { SCENE SIXIESME. }
  \livretDescAtt \wordwrap-center { ATYS, SANGARIDE. }
  \livretRef #'BFAatysSangaride
  \livretPers ATYS.
  \livretVer { Sangaride, ce jour est un grand jour pour vous. }

  \livretPers SANGARIDE.
  \livretVer { Nous ordonnons tous deux la feste de Cybele, }
  \livretVerC { L'honneur est égal entre nous. }

  \livretPers ATYS.
  \livretVer { Ce jour mesme, un grand Roy doit estre vostre espoux, }
  \livretVer { Je ne vous vis jamais si contente et si belle ; }
  \livretVerC { Que le sort du Roy sera doux ! }

  \livretPers SANGARIDE.
  \livretVer { L'indifferent Atys n'en sera point jaloux. }

  \livretPers ATYS.
  \livretVer { Vivez tous deux contens, c'est ma plus chere envie ; }
  \livretVer { J'ay pressé vostre hymen, j'ay servy vos amours. }
  \livretVer { Mais enfin ce grand jour, le plus beau de vos jours, }
  \livretVerC { Sera le dernier de ma vie. }

  \livretPers SANGARIDE.
  \livretVer { O dieux ! }

  \livretPers ATYS.
  \livretVer { \invisible { O dieux ! }
    Ce n'est qu'à vous que je veux reveler }
  \livretVer { Le secret desespoir où mon malheur me livre ; }
  \livretVer { Je n'ay que trop sceu feindre, il est temps de parler ; }
  \livretVerC { Qui n'a plus qu'un moment à vivre, }
  \livretVerC { N'a plus rien à dissimuler. }

  \livretPers SANGARIDE.
  \livretVerC { Je fremis, ma crainte est extresme ; }
  \livretVer { Atys, par quel malheur faut-il vous voir perir ? }

  \livretPers ATYS.
  \livretVerC { Vous me condamnerez vous mesme, }
  \livretVerC { Et vous me laisserez mourir. }

  \livretPers SANGARIDE.
  \livretVer { J'armeray, s'il se faut, tout le pouvoir supresme... }

  \livretPers ATYS.
  \livretVerC { Non, rien ne peut me secourir, }
  \livretVer { Je meurs d'amour pour vous, je n'en sçaurois guerir ; }

  \livretPers SANGARIDE.
  \livretVer { Quoy ? vous ? }

  \livretPers ATYS.
  \livretVer { \invisible { Quoy ? vous ? } Il est trop vray. }

  \livretPers SANGARIDE.
  \livretVer { \invisible { Quoy ? vous ? Il est trop vray. } Vous m'aimez ? }

  \livretPers ATYS.
  \livretVer { \invisible { Quoy ? vous ? Il est trop vray. Vous m'aimez ? } Je vous aime. }
  \livretVerC { Vous me condamnerez vous mesme, }
  \livretVerC { Et vous me laisserez mourir. }
  \livretVerC { J'ay merité qu'on me punisse, }
  \livretVerC { J'offence un Rival genereux, }
  \livretVer { Qui par mille bien-faits a prevenu mes vœux : }
  \livretVer { Mais je l'offence en vain, vous luy rendez justice ; }
  \livretVerC { Ah ! que c'est un cruel suplice }
  \livretVer { D'avoüer qu'un Rival est digne d'estre heureux ! }
  \livretVer { Prononcez mon arrest, parlez sans vous contraindre. }

  \livretPers SANGARIDE.
  \livretVer { Helas ! }

  \livretPers ATYS.
  \livretVer { \invisible { Helas ! }
    Vous soûpirez ? je voy couler vos pleurs ? }
  \livretVer { D'un malheureux amour plaignez-vous les douleurs ? }

  \livretPers SANGARIDE.
  \livretVerC { Atys, que vous seriez à plaindre }
  \livretVerC { Si vous sçaviez tous vos malheurs ! }

  \livretPers ATYS.
  \livretVerC { Si je vous pers, et si je meurs, }
  \livretVerC { Que puis-je encore avoir à craindre ? }

  \livretPers SANGARIDE.
  \livretVer { C'est peu de perdre en moy ce qui vous a charmé, }
  \livretVer { Vous me perdez, Atys, et vous estes aimé. }

  \livretPers ATYS.
  \livretVer { Aimé ! qu'entens-je ? ô Ciel ! quel aveu favorable ! }

  \livretPers SANGARIDE.
  \livretVerC { Vous en serez plus miserable. }

  \livretPers ATYS.
  \livretVerC { Mon malheur en est plus affreux, }
  \livretVer { Le bonheur que je pers doit redoubler ma rage ; }
  \livretVer { Mais n'importe, aimez-moy, s'il se peut, d'avantage, }
  \livretVer { Quand j'en devrois mourir cent fois plus malheureux. }

  \livretPers SANGARIDE.
  \livretVer { Si vous cherchez la mort, il faut que je vous suive ; }
  \livretVer { Vivez, c'est mon amour qui vous en fait la loy. }

  \livretPers ATYS.
  \livretVerC { Hé comment ! hé pourquoy }
  \livretVerC { voulez-vous que je vive, }
  \livretVerC { Si vous ne vivez pas pour moy ? }

  \livretPers \line { ATYS & SANGARIDE. }
  \livretVer { Si l'Hymen unissoit mon destin et le vostre, }
  \livretVerC { Que ses nœuds auroient eû d'attraits ! }
  \livretVerC { L'Amour fit nos cœurs l'un pour l'autre, }
  \livretVer { Faut-il que le devoir les separe à jamais ? }

  \livretPers ATYS.
  \livretVerC { Devoir impitoyable ! }
  \livretVerC { Ah quelle cruauté ! }

  \livretPers SANGARIDE.
  \livretVer { On vient, feignez encor, craignez d'estre écouté. }

  \livretPers ATYS.
  \livretVerC { Aimons un bien plus durable }
  \livretVerC { Que l'éclat de la Beauté : }
  \livretVerC { Rien n'est plus aimable }
  \livretVerC { Que la liberté. }

  %%%
  \livretScene \line { SCENE SEPTIESME. }
  \livretDescAtt \column {
    \wordwrap-center { ATYS, SANGARIDE, DORIS, IDAS. }
    \line { Chœur de Phrygiens chantans. Chœur de Phrygiennes chantantes. }
    \line { Troupe de Phrygiens dançans. Troupe de Phrygiennes dançantes. }
    \justify\italic {
      Dix Hommes Phrygiens chantans conduits par Atys.
      Dix femmes Phrygiennes chantãtes conduites par Sangaride.
      Six Phrygiens dançans.
      Six Nimphes Phrygiennes dançantes.
    }
  }
  \livretRef #'BGAatysSangarideChoeur
  \livretPers ATYS.
  \livretVerC { Mais déja de ce Mont sacré }
  \livretVerC { Le sommet paroist éclairé }
  \livretVerC { D'une splendeur nouvelle. }

  \livretPers \line { SANGARIDE s'avançant vers la Montagne. }
  \livretVer { Le Déesse descend, allons au devant d'elle. }

  \livretPers \line { ATYS & SANGARIDE. }
  \livretVerC { Commençons, commençons }
  \livretVer { De celebrer icy sa feste solemnelle, }
  \livretVerC { Commençons, commençons }
  \livretVerC { Nos Jeux et nos chansons. }
  \livretDesc \wordwrap { Les chœurs repetent ces derniers Vers. }

  \livretPers \line { ATYS & SANGARIDE. }
  \livretVer { Il est temps que chacun fasse éclater son zele. }
  \livretVerC { Venez, Reine des Dieux, venez, }
  \livretVerC { Venez, favorable Cybele. }
  \livretDesc \wordwrap { Les chœurs repetent ces deux derniers Vers. }

  \livretPers ATYS.
  \livretVerC { Quittez vostre Cour immortelle, }
  \livretVerC { Choisissez ces lieux fortunez }
  \livretVerC { Pour vostre demeure éternelle. }

  \livretPers \line { Les Chœurs. }
  \livretVerC { Venez, Reine des Dieux, venez, }

  \livretPers SANGARIDE.
  \livretVer { La Terre sous vos pas va devenir plus belle }
  \livretVer { Que le sejour des Dieux que vous abandonnez. }

  \livretPers \line { Les Chœurs. }
  \livretVerC { Venez, favorable Cybele. }

  \livretPers \line { ATYS & SANGARIDE. }
  \livretVer { Venez voir les Autels qui vous sont destinez. }

  \livretPers \line { ATYS, SANGARIDE, IDAS, DORIS, & les Chœurs. }
  \livretVerC { Eoutez un Peuple fidelle }
  \livretVerC { Qui vous appelle, }
  \livretVerC { Venez Reine des Dieux, venez, }
  \livretVerC { Venez favorable Cybele. }

  %%%
  \livretScene \line { SCENE HUITIESME. }
  \livretDescAtt \wordwrap {
    La Déesse Cybele paroist sur son Char, &
    les Phrygiens & les Phrygiennes luy témoignent
    leur joye & leur respect.
  }
  \livretRef #'BHBcybeleChoeur
  \livretPers \line { CYBELE sur son Char. }
  \livretVer { Venez tous dans mon Temple, et que chacun revere }
  \livretVer { Le sacrificateur dont je vais faire choix : }
  \livretVerC { Je m'expliqueray par sa voix, }
  \livretVer { Les vœux qu'il m'offrira seront seurs de me plaire. }
  \livretVer { Je reçoy vos respects ; j'aime à voir les honneurs }
  \livretVer { Dont vous me presentez un éclatant hommage, }
  \livretVerC { Mais l'hommage des Cœurs }
  \livretVerC { Est ce que j'aime davantage. }
  \livretVerC { Vous devez vous animer }
  \livretVerC { D'une ardeur nouvelle, }
  \livretVerC { S'il faut honorer Cybele, }
  \livretVerC { Il faut encor plus l'aimer. }
  
  \livretDesc \wordwrap {
    CYBELE portée par son Char volant, se
    va rendre dans son Temple. Tous les Phrygiens
    s'empressent d'y aller, & repetent les quatres
    derniers Vers que la Déesse a prononcez.
  }

  \livretPers \line { Les Chœurs. }
  \livretVerC { Nous devons nous animer }
  \livretVerC { D'une ardeur nouvelle, }
  \livretVerC { S'il faut honorer Cybele, }
  \livretVerC { Il faut encor plus l'aimer. }

  \livretFinAct \line { Fin du premier Acte. }
}
