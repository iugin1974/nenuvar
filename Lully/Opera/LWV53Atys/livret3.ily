\markup\n-columns {
  \livretAct \line { ACTE TROISIESME. }
  \livretDescAtt \wordwrap-center {
    Le Theatre change & represente
    le Palais du Sacrificateur
    de Cybele.
  }
  \livretScene \line { SCENE PREMIERE. }
  \livretRef #'DAAatys
  \livretPers \line { Atys seul. }
  \livretVer { Que servent les faveurs que nous fait la Fortune }
  \livretVerC { Quãd l'Amour nous rend malheureux ? }
  \livretVer { Je pers l'unique bien qui peut combler mes vœux, }
  \livretVerC { Et tout autre bien m'importune. }
  \livretVer { Que servent les faveurs que nous fait la Fortune }
  \livretVerC { Quand l'Amour nous rend malheureux ? }

  %%
  \livretScene \line { SCENE SECONDE. }
  \livretDescAtt \wordwrap-center { IDAS, DORIS, ATYS. }
  \livretRef #'DBAatysIdasDoris
  \livretPers IDAS.
  \livretVerC { Peut-on icy parler sans feindre ?  }

  \livretPers ATYS.
  \livretVer { Je commande en ces lieux, vous n'y devez rien craindre. }

  \livretPers DORIS.
  \livretVer { Mon frere est votre amy. }

  \livretPers IDAS.
  \livretVer { \invisible { Mon frere est votre amy. } Fiez-vous à ma sœur. }

  \livretPers ATYS.
  \livretVer { Vous devez avec moy partager mon bon-heur. }

  \livretPers \line { IDAS, & DORIS. }
  \livretVer { Nous venons partager vos mortelles allarmes ; }
  \livretVerC { Sangaride les yeux en larmes }
  \livretVerC { Nous vient d'ouvrir son cœur. }

  \livretPers ATYS.
  \livretVer { L'heure aproche où l'Hymen voudra qu'elle se livre }
  \livretVerC { Au pouvoir d'un heureux espoux. }

  \livretPers \line { IDAS, & DORIS. }
  \livretVerC { Elle ne peut vivre }
  \livretVerC { Pour un autre que pour vous. }

  \livretPers ATYS.
  \livretVer { Qui peut la dégager du devoir qui la presse ? }

  \livretPers \line { IDAS, & DORIS. }
  \livretVer { Elle veut elle mesme aux pieds de la Deesse }
  \livretVer { Declarer hautement vos secretes amours. }

  \livretPers ATYS.
  \livretVerC { Cybele pour moy s'interesse, }
  \livretVer { J'ose tout esperer de son divin secours... }
  \livretVer { Mais quoy, trahir le Roy ! tromper son esperance ! }
  \livretVer { De tant de biens receus est-ce la recompense ? }

  \livretPers \line { IDAS, & DORIS. }
  \livretVerC { Dans l'Empire amoureux }
  \livretVerC { Le Devoir n'a point de puissance ; }
  \livretVerC { L'Amour dispence }
  \livretVerC { Les Rivaux d'estre genereux ; }
  \livretVerC { Il faut souvent pour devenir heureux }
  \livretVerC { Qu'il en couste un peu d'innocence. }

  \livretPers ATYS.
  \livretVer { Je souhaite, je crains, je veux, je me repens. }

  \livretPers \line { IDAS, & DORIS. }
  \livretVer { Verrez-vous un rival heureux à vos dépens ? }

  \livretPers ATYS.
  \livretVer { Je ne puis me resoudre à cette violence. }

  \livretPers \line { ATYS, IDAS, & DORIS. }
  \livretVer { En vain, un cœur, incertain de son choix, }
  \livretVerC { Met en balance mille fois }
  \livretVerC { L'Amour et la Reconnoissance, }
  \livretVerC { L'Amour toûjours emporte la balance. }

  \livretPers ATYS.
  \livretVer { Le plus juste party cede enfin au plus fort. }
  \livretVerC { Allez, prenez soin de mon sort, }
  \livretVer { Que Sangaride icy se rende en diligence. }

  %%
  \livretScene \line { SCENE TROISIESME. }
  \livretRef #'DCAatys
  \livretPers \line { ATYS seul. }
  \livretVer { Nous pouvons nous flater de l'espoir le plus doux }
  \livretVerC { Cybele et l'Amour sont pour nous. }
  \livretVer { Mais du Devoir trahi j'entends la voix pressante }
  \livretVerC { Qui m'accuse et qui m'épouvante. }
  \null
  \livretVer { Laisse mon cœur en paix, impuissante Vertu, }
  \livretVerC { N'ay-je point assez combatu ? }
  \livretVer { Quand l'Amour malgré toy me contraint à me rendre, }
  \livretVerC { Que me demandes-tu ? }
  \livretVerC { Puisque tu ne peux me deffendre, }
  \livretVerC { Que me sert-il d'entendre }
  \livretVerC { Les vains reproches que tu fais ? }
  \livretVer { Impuissante Vertu laisse mon cœur en paix. }
  \null
  \livretVerC { Mais le Sommeil vient me surprendre, }
  \livretVer { Je combats vainement sa charmante douceur. }
  \livretVerC { Il faut laisser suspendre }
  \livretVerC { Les troubles de mon cœur. }

  \livretDidasP \line { Atys descend. }

  %%
  \livretScene \line { SCENE QUATRIESME. }
  \livretDescAtt \justify {
    Le Theatre change & represente un Antre
    entouré de Pavots & de Ruisseaux, où le
    Dieux du Sommeil se vient rendre accompagné
    des Songes agreables & funestes.
  }
  \livretDescAtt \column {
    \wordwrap-center {
      ATYS dormant. LE SOMMEIL, MORPHÉE,
      PHOBETOR, PHANTASE,
      Les Songes heureux. Les Songes funestes.
    }
    \justify\italic {
      Deux Songes joüants de la Violle.
      Deux Songes joüants du Theorbe.
      Six Songes joüants de la Flutte.
      Douze Songes funestes chantants.
      Huit Songes agreables dançans.
      Huit Songes funestes dançants.
    }
  }
  \livretRef #'DDAsommeil
  \livretPers \line { LE SOMMEIL. }
  \livretVerC { Dormons, dormons tous ; }
  \livretVerC { Ah que le repos est doux ! }

  \livretPers MORPHÉE.
  \livretVer { Regnez, divin Sommeil, regnez sur tout le monde, }
  \livretVer { Répandez vos pavots les plus assoupissans ; }
  \livretVerC { Calmez les soins, charmez les sens, }
  \livretVer { Retenez tous les cœurs dans une paix profonde. }

  \livretPers PHOBETOR.
  \livretVerC { Ne vous faites point violence, }
  \livretVerC { Coulez, murmurez, clairs Ruisseaux, }
  \livretVerC { Il n'est permis qu'au bruit des eaux }
  \livretVer { De troubler la douceur d'un si charmant silence. }
}
\markup\n-columns {
  \livretPers \line { LE SOMMEIL, MORPHÉE, PHOBETOR & PHANTASE }
  \livretVerC { Dormons, dormons tous, }
  \livretVerC { Ah que le repos est doux ! }

  \livretDesc\justify {
    Les Songes agreables aprochent d'Atys, &
    par leurs chants, & par leurs dances, luy font
    connoistre l'amour de Cybele, & e bonheur
    qu'il en doit esperer.
  }
  \livretRef #'DDBsommeil
  \livretPers MORPHÉE.
  \livretVer { Escoute, escoute Atys la gloire qui t'appelle, }
  \livretVer { Sois sensible à l'hõneur d'estre aymé de Cybelle, }
  \livretVerC { Joüis heureux Atys de ta felicité. }

  \livretPers \line { MORPHÉE, PHOBETOR & PHANTASE }
  \livretVerC { Mais souvien-toy que la Beauté }
  \livretVerC { Quand elle est immortelle, }
  \livretVerC { Demande la fidelité }
  \livretVerC { D'une amour éternelle. }

  \livretPers PHANTASE.
  \livretVerC { Que l'Amour a d'attraits }
  \livretVerC { Lors qu'il commence }
  \livretVerC { A faire sentir sa puissance, }
  \livretVerC { Que l'Amour a d'attraits }
  \livretVerC { Lors qu'il commence }
  \livretVerC { Pour ne finir jamais. }
  \null
  \livretVerC { Trop heureux un Amant }
  \livretVerC { Qu'amour exemte }
  \livretVerC { Des peines d'une longue attente ! }
  \livretVerC { Trop heureux un Amant }
  \livretVerC { Qu'amour exemte }
  \livretVerC { De crainte, et de tourment ! }

  \livretRef #'DDDsommeil
  \livretPers MORPHÉE.
  \livretVer { Gouste en paix chaque jour une douceur nouvelle, }
  \livretVer { Partage l'heureux sort d'une Divinité, }
  \livretVerC { Ne vante plus la liberté, }
  \livretVer { Il n'est point du prix d'une chaîne si belle : }

  \livretPers \line { MORPHÉE, PHOBETOR & PHANTASE }
  \livretVerC { Mais souvien-toy que la Beauté }
  \livretVerC { Quand elle est immortelle, }
  \livretVerC { Demande la fidelité }
  \livretVerC { D'une amour éternelle. }

  \livretPers PHANTASE.
  \livretVerC { Que l'Amour a d'attraits }
  \livretVerC { Lors qu'il commence }
  \livretVerC { A faire sentir sa puissance, }
  \livretVerC { Que l'Amour a d'attraits }
  \livretVerC { Lors qu'il commence }
  \livretVerC { Pour ne finir jamais. }

  \livretDesc\justify {
    Les songes funestes approchent d'Atys, & le
    menacent de la vengeance de Cybele s'il mesprise
    son amour, & s'il ne l'ayme pas avec fidelité.
  }
  \livretRef #'DDFfuneste
  \livretPers \line { UN SONGE FUNESTE. }
  \livretVer { Garde-toy d'offencer un amour glorieux, }
  \livretVer { C'est pour toy que Cybele abandonne les Cieux }
  \livretVerC { Ne trahis point son esperance. }
  \livretVer { Il n'est point pour les Dieux de mespris innocent, }
  \livretVer { Ils sont jaloux des Cœurs, ils ayment la vengeance, }
  \livretVerC { Il est dangereux qu'on offence }
  \livretVerC { Un amour tout-puissant. }

  \livretRef #'DDHchoeur
  \livretPers \line { CHŒUR DE SONGES FUNESTES. }
  \livretVerC { L'amour qu'on outrage }
  \livretVerC { Se transforme en rage, }
  \livretVerC { Et ne pardonne pas }
  \livretVerC { Aux plus charmants appas. }
  \livretVerC { Si tu n'aymes point Cybele }
  \livretVerC { D'une amour fidelle, }
  \livretVerC { Malheureux, que tu souffriras ! }
  \livretVerC { Tu periras : }
  \livretVerC { Crains une vengeance cruelle, }
  \livretVerC { Tremble, crains un affreux trépas. }

  \livretDesc \justify {
    Atys espouvanté par les Songes funestes, se
    resveille en sursaut, le Sommeil & les Songes
    disparoissent avec l'Antre où ils estoient, & Atys
    se retrouve dans le mesme Palais où il s'estoit
    endormy.
  }
  %%
  \livretScene \line { SCENE CINQUIESME. }
  \livretDescAtt \wordwrap-center { ATYS, CYBELE, & MELISSE. }
  \livretRef #'DEAcybeleAtys
  \livretPers ATYS.
  \livretVer { Venez à mon secours ô Dieux ! ô justes Dieux ! }

  \livretPers CYBELE.
  \livretVer { Atys, ne craignez rien, Cybele, est en ces lieux. }

  \livretPers ATYS.
  \livretVer { Pardonnez au desordre où mon cœur s'abandonne ; }
  \livretVer { C'est un songe... }

  \livretPers CYBELE.
  \livretVer { \invisible { C'est un songe... } Parlez, quel songe vous estonne ? }
  \livretVerC { Expliquez moy vostre embaras. }

  \livretPers ATYS.
  \livretVer { Les songes sont trompeurs, et je ne les croy pas. }
  \livretVerC { Les plaisirs et les peines }
  \livretVerC { Dont en dormant on est séduit, }
  \livretVerC { Sont des chimeres vaines }
  \livretVerC { Que le resveil détruit. }

  \livretPers CYBELE.
  \livretVerC { Ne mesprisez pas tant les songes }
  \livretVerC { L'Amour peut souvent emprunter leur voix, }
  \livretVerC { S'ils font souvent des mensonges }
  \livretVerC { Ils disent vray quelquefois. }
  \livretVer { Ils parloient par mon ordre, et vous les devez croire. }

  \livretPers ATYS.
  \livretVer { O Ciel ? }

  \livretPers CYBELE.
  \livretVer { \invisible { O Ciel ? } N'en doutez point, connoissez vostre gloire. }
  \livretVerC { Respondez avec liberté, }
  \livretVer { Je vous demãde un cœur qui despend de luy-mesme. }

  \livretPers ATYS.
  \livretVerC { Une grande Divinité }
  \livretVer { Doit d'assûrer toûjours de mon respect extresme. }

  \livretPers CYBELE.
  \livretVerC { Les Dieux dans leur grandeur supresme }
  \livretVer { Reçoivent tant d'honneurs qu'ils en sont rebutez, }
  \livretVer { Ils se lassent souvent d'estre trop respectez, }
  \livretVerC { Ils sont plus contents qu'on les aymes. }

  \livretPers ATYS.
  \livretVerC { Je sçay trop ce que je vous doy }
  \livretVerC { Pour manquer de reconnoissance... }

}
\markup\n-columns {
  %%
  \livretScene \line { SCENE SIXIESME. }
  \livretDescAtt \wordwrap-center { SANGARIDE, CYBELE, ATYS, MELISSE. }
  \livretRef #'DFAcybeleAtysSangaride
  \livretPers \line { SANGARIDE se jettant aux pieds de Cybele. }
  \livretVerC { J'ay recours à vostre puissance, }
  \livretVerC { Reyne des Dieux, protegez-moy. }
  \livretVerC { L'interest d'Atys vous en presse... }

  \livretPers \line { ATYS interrompant Sangaride. }
  \livretVer { Je parleray pour vous, que vostre crainte cesse. }

  \livretPers SANGARIDE.
  \livretVerC { Tous deux unis des plus beaux nœuds... }

  \livretPers \line { ATYS interrompant Sangaride. }
  \livretVer { Le sang et l'amitié nous unissent tous deux. }
  \livretVerC { Que vostre secours la délivre }
  \livretVerC { Des loix d'un Hymen rigoureux, }
  \livretVerC { Ce sont les plus doux de ses vœux }
  \livretVer { De pouvoir à jamais vous servir et vous suivre. }

  \livretPers CYBELE.
  \livretVerC { Les Dieux sont les protecteurs }
  \livretVerC { De la liberté des cœurs. }
  \livretVer { Allez, ne craignez point le Roy ny sa colere, }
  \livretVerC { J'auray soin d'appaiser }
  \livretVerC { Le Fleuve Sangar vostre Pere ; }
  \livretVerC { Atys veut vous favoriser, }
  \livretVer { Cybele en sa faveur ne peut rien refuser. }

  \livretPers ATYS.
  \livretVer { Ah ! c'en est trop... }

  \livretPers CYBELE.
  \livretVer { \invisible { Ah ! c'en est trop... } Non, non, il n'est pas necessaire }
  \livretVerC { Que vous cachiez vostre bonheur, }
  \livretVerC { Je ne prétens point faire }
  \livretVerC { Un vain mystere }
  \livretVerC { D'un amour qui vous fait honneur. }
  \livretVer { Ce n'est point à Cybelle à craindre d'en trop dire. }
  \livretVer { Il est vray, j'ayme Atys, pour luy j'ay tout quitté, }
  \livretVer { Sans luy je ne veux plus de grandeur ny d'Empire, }
  \livretVerC { Pour ma felicité }
  \livretVerC { Son cœur seul peut suffire. }
  \livretVer { Allez, Atys luy-mesme ira vous garentir }
  \livretVerC { De la fatale violence }
  \livretVerC { Où vous ne pouvez consentir. }

  \livretDidasP \line { Sangaride se retire. }
  \livretPers \line { CYBELE parle à Atys. }
  \livretVer { Laissez-nous, attendez mes ordres pour partir, }
  \livretVer { Je prétens vous armer de ma toute-puissance. }

  %%
  \livretScene \line { SCENE SEPTIESME. }
  \livretDescAtt \wordwrap-center { CYBELE, MELISSE. }
  \livretRef #'DGAcybeleMelisse
  \livretPers CYBELE.
  \livretVer { Qu'Atys dans ses respects mesle d'indifference ! }
  \livretVerC { L'Ingrat Atys ne m'ayme pas ; }
  \livretVer { L'Amour veut de l'amour, tout autre prix l'offence, }
  \livretVer { Et souvent le respect et la reconnoissance }
  \livretVerC { Sont l'excuse des cœurs ingrats. }

  \livretPers MELISSE.
  \livretVerC { Ce n'est pas un si grand crime }
  \livretVerC { De ne s'exprimer pas bien, }
  \livretVerC { Un cœur qui n'ayma jamais rien }
  \livretVerC { Sçait peu comment l'amour s'exprime. }

  \livretPers CYBELE.
  \livretVer { Sangaride est aymable, Atys peut tout charmer, }
  \livretVerC { Ils tesmoignent trop s'estimer, }
  \livretVer { Et de simples parents sont moins d'intelligence : }
  \livretVerC { Ils se sont aymez dès l'enfance, }
  \livretVerC { Ils pourroient enfin trop s'aymer. }
  \livretVer { Je crains une amitié que tant d'ardeur anime. }
  \livretVerC { Rien n'est si trompeur que l'estime : }
  \livretVerC { C'est un nom supposé }
  \livretVer { Qu'on donne quelquefois à l'amour desguisé. }
  \livretVer { Je prétens m'esclaircir leur feinte sera vaine. }

  \livretPers MELISSE.
  \livretVer { Quels secrets par les Dieux ne sont point penetrez ? }
  \livretVerC { Deux cœurs à feindre preparez }
  \livretVerC { Ont beau cacher leur chaîne, }
  \livretVerC { On abuse avec peine }
  \livretVerC { Les Dieux par l'Amour esclairez. }

  \livretPers CYBELE.
  \livretVer { Va, Melisse, donne ordre à l'aymable Zephire }
  \livretVer { D'accomplir promptement tout ce qu'Atys desire. }

  %%
  \livretScene \line { SCENE HUITIESME. }
  \livretRef #'DHAcybele
  \livretPers \line { CYBELE seule. }
  \livretVerC { Espoir si cher, et si doux, }
  \livretVerC { Ah ! pourquoy me trompez-vous ? }
  \livretVer { Des suprêmes grãdeurs vous m'avez fait descendre, }
  \livretVer { Mille Cœurs m'adoroient, je les neglige tous, }
  \livretVer { Je n'en demande qu'un, il a peine à se rendre ; }
  \livretVer { Je ne sens que chagrin, et que soupçons jaloux ; }
  \livretVer { Est-ce le sort charmant que je devois attendre ? }
  \livretVerC { Espoir si cher, et si doux, }
  \livretVerC { Ah ! pourquoy me trompez-vous ? }
  \livretVer { Helas ! par tant d'attraits falloit-il me surprendre ? }
  \livretVer { Heureuse, si toûjours j'avois pû m'en deffendre ! }
  \livretVer { L'Amour qui me flattoit me cachoit son couroux : }
  \livretVer { C'est donc pour me fraper des plus funestes coups, }
  \livretVer { Que le cruel Amour m'a fait un cœur si tendre ? }
  \livretVerC { Espoir si cher, et si doux, }
  \livretVerC { Ah ! pourquoy me trompez-vous ? }
  \sep
  \vspace #28
}
