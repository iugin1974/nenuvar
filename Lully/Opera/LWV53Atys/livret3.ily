\notesSubSection "Acte III"
\markuplines \column-lines {
  \livretAct \line { ACTE TROISIESME. }
  \livretDesc \wordwrap-center {
    Le Theatre change & represente
    le Palais du Sacrificateur
    de Cybele.
  }
  \livretScene \line { SCENE PREMIERE. }
  \livretPersRef #'DAAatys \line { Atys seul. }
  \livretVers {
    \line { Que servent les faveurs que nous fait la Fortune }
    \sline { Quãd l'Amour nous rend malheureux ? }
    \line { Je pers l'unique bien qui peut combler mes vœux, }
    \sline { Et tout autre bien m'importune. }
    \line { Que servent les faveurs que nous fait la Fortune }
    \sline { Quand l'Amour nous rend malheureux ? }
  }
  %%
  \livretScene \line { SCENE SECONDE. }
  \livretDesc \wordwrap-center { IDAS, DORIS, ATYS. }
  \livretPersRef #'DBAatysIdasDoris IDAS.
  \livretVers {
    \sline { Peut-on icy parler sans feindre ?  }
  }
  \livretPers ATYS.
  \livretVers {
    \line { Je commande en ces lieux, vous n'y devez rien craindre. }
  }
  \livretPers DORIS.
  \livretVers {
    \line { Mon frere est votre amy. }
  }
  \livretPers IDAS.
  \livretVers {
    \line { \invisible { Mon frere est votre amy. } Fiez-vous à ma sœur. }
  }
  \livretPers ATYS.
  \livretVers {
    \line { Vous devez avec moy partager mon bon-heur. }
  }
  \livretPers \line { IDAS, & DORIS. }
  \livretVers {
    \line { Nous venons partager vos mortelles allarmes ; }
    \sline { Sangaride les yeux en larmes }
    \sline { Nous vient d'ouvrir son cœur. }
  }
  \livretPers ATYS.
  \livretVers {
    \line { L'heure aproche où l'Hymen voudra qu'elle se livre }
    \sline { Au pouvoir d'un heureux espoux. }
  }
  \livretPers \line { IDAS, & DORIS. }
  \livretVers {
    \sline { Elle ne peut vivre }
    \sline { Pour un autre que pour vous. }
  }
  \livretPers ATYS.
  \livretVers {
    \line { Qui peut la dégager du devoir qui la presse ? }
  }
  \livretPers \line { IDAS, & DORIS. }
  \livretVers {
    \line { Elle veut elle mesme aux pieds de la Deesse }
    \line { Declarer hautement vos secretes amours. }
  }
  \livretPers ATYS.
  \livretVers {
    \sline { Cybele pour moy s'interesse, }
    \line { J'ose tout esperer de son divin secours... }
    \line { Mais quoy, trahir le Roy ! tromper son esperance ! }
    \line { De tant de biens receus est-ce la recompense ? }
  }
  \livretPers \line { IDAS, & DORIS. }
  \livretVers {
    \sline { Dans l'Empire amoureux }
    \sline { Le Devoir n'a point de puissance ; }
    \sline { L'Amour dispence }
    \sline { Les Rivaux d'estre genereux ; }
    \sline { Il faut souvent pour devenir heureux }
    \sline { Qu'il en couste un peu d'innocence. }
  }
  \livretPers ATYS.
  \livretVers {
    \line { Je souhaite, je crains, je veux, je me repens. }
  }
  \livretPers \line { IDAS, & DORIS. }
  \livretVers {
    \line { Verrez-vous un rival heureux à vos dépens ? }
  }
  \livretPers ATYS.
  \livretVers {
    \line { Je ne puis me resoudre à cette violence. }
  }
  \livretPers \line { ATYS, IDAS, & DORIS. }
  \livretVers {
    \line { En vain, un cœur, incertain de son choix, }
    \sline { Met en balance mille fois }
    \sline { L'Amour et la Reconnoissance, }
    \sline { L'Amour toûjours emporte la balance. }
  }
  \livretPers ATYS.
  \livretVers {
    \line { Le plus juste party cede enfin au plus fort. }
    \sline { Allez, prenez soin de mon sort, }
    \line { Que Sangaride icy se rende en diligence. }
  }
  %%
  \livretScene \line { SCENE TROISIESME. }
  \livretPersRef #'DCAatys \line { ATYS seul. }
  \livretVers {
    \line { Nous pouvons nous flater de l'espoir le plus doux }
    \sline { Cybele et l'Amour sont pour nous. }
    \line { Mais du Devoir trahi j'entends la voix pressante }
    \sline { Qui m'accuse et qui m'épouvante. }
    \null
    \line { Laisse mon cœur en paix, impuissante Vertu, }
    \sline { N'ay-je point assez combatu ? }
    \line { Quand l'Amour malgré toy me contraint à me rendre, }
    \sline { Que me demandes-tu ? }
    \sline { Puisque tu ne peux me deffendre, }
    \sline { Que me sert-il d'entendre }
    \sline { Les vains reproches que tu fais ? }
    \line { Impuissante Vertu laisse mon cœur en paix. }
    \null
    \sline { Mais le Sommeil vient me surprendre, }
    \line { Je combats vainement sa charmante douceur. }
    \sline { Il faut laisser suspendre }
    \sline { Les troubles de mon cœur. }
  }
  \livretDidas \line { Atys descend. }
  %%
  \livretScene \line { SCENE QUATRIESME. }
  \livretDesc \column {
    \justify {
      Le Theatre change & represente un Antre
      entouré de Pavots & de Ruisseaux, où le
      Dieux du Sommeil se vient rendre accompagné
      des Songes agreables & funestes.
    }
    \justify {
      ATYS dormant. LE SOMMEIL, MORPHÉE,
      PHOBETOR, PHANTASE,
      Les Songes heureux. Les Songes funestes.
    }
    \wordwrap { Deux Songes joüants de la Violle. }
    \wordwrap { Deux Songes joüants du Theorbe. }
    \wordwrap { Six Songes joüants de la Flutte. }
    \wordwrap { Douze Songes funestes chantants. }
    \wordwrap { Huit Songes agreables dançans. }
    \wordwrap { Huit Songes funestes dançants. }
  }
  \livretPersRef #'DDAsommeil \line { LE SOMMEIL. }
  \livretVers {
    \sline { Dormons, dormons tous ; }
    \sline { Ah que le repos est doux ! }
  }
  \livretPers MORPHÉE.
  \livretVers {
    \line { Regnez, divin Sommeil, regnez sur tout le monde, }
    \line { Répandez vos pavots les plus assoupissans ; }
    \sline { Calmez les soins, charmez les sens, }
    \line { Retenez tous les cœurs dans une paix profonde. }
  }
  \livretPers PHOBETOR.
  \livretVers {
    \sline { Ne vous faites point violence, }
    \sline { Coulez, murmurez, clairs Ruisseaux, }
    \sline { Il n'est permis qu'au bruit des eaux }
    \line { De troubler la douceur d'un si charmant silence. }
  }
  \livretPers \line { LE SOMMEIL, MORPHÉE, PHOBETOR & PHANTASE }
  \livretVers {
    \sline { Dormons, dormons tous, }
    \sline { Ah que le repos est doux ! }
  }
  \livretDesc \wordwrap {
    Les Songes agreables aprochent d'Atys, &
    par leurs chants, & par leurs dances, luy font
    connoistre l'amour de Cybele, & e bonheur
    qu'il en doit esperer.
  }
  \livretPersRef #'DDBsommeil MORPHÉE.
  \livretVers {
    \line { Escoute, escoute Atys la gloire qui t'appelle, }
    \line { Sois sensible à l'hõneur d'estre aymé de Cybelle, }
    \sline { Joüis heureux Atys de ta felicité. }
  }
  \livretPers \line { MORPHÉE, PHOBETOR & PHANTASE }
  \livretVers {
    \sline { Mais souvien-toy que la Beauté }
    \sline { Quand elle est immortelle, }
    \sline { Demande la fidelité }
    \sline { D'une amour éternelle. }
  }
  \livretPers PHANTASE.
  \livretVers {
    \sline { Que l'Amour a d'attraits }
    \sline { Lors qu'il commence }
    \sline { A faire sentir sa puissance, }
    \sline { Que l'Amour a d'attraits }
    \sline { Lors qu'il commence }
    \sline { Pour ne finir jamais. }
    \null
    \sline { Trop heureux un Amant }
    \sline { Qu'amour exemte }
    \sline { Des peines d'une longue attente ! }
    \sline { Trop heureux un Amant }
    \sline { Qu'amour exemte }
    \sline { De crainte, et de tourment ! }
  }
  \livretPersRef #'DDDsommeil MORPHÉE.
  \livretVers {
    \line { Gouste en paix chaque jour une douceur nouvelle, }
    \line { Partage l'heureux sort d'une Divinité, }
    \sline { Ne vante plus la liberté, }
    \line { Il n'est point du prix d'une chaîne si belle : }
  }
  \livretPers \line { MORPHÉE, PHOBETOR & PHANTASE }
  \livretVers {
    \sline { Mais souvien-toy que la Beauté }
    \sline { Quand elle est immortelle, }
    \sline { Demande la fidelité }
    \sline { D'une amour éternelle. }
  }
  \livretPers PHANTASE.
  \livretVers {
    \sline { Que l'Amour a d'attraits }
    \sline { Lors qu'il commence }
    \sline { A faire sentir sa puissance, }
    \sline { Que l'Amour a d'attraits }
    \sline { Lors qu'il commence }
    \sline { Pour ne finir jamais. }
  }
  \livretDesc \wordwrap {
    Les songes funestes approchent d'Atys, & le
    menacent de la vengeance de Cybele s'il mesprise
    son amour, & s'il ne l'ayme pas avec fidelité.
  }
  \livretPersRef #'DDFfuneste \line { UN SONGE FUNESTE. }
  \livretVers {
    \line { Garde-toy d'offencer un amour glorieux, }
    \line { C'est pour toy que Cybele abandonne les Cieux }
    \sline { Ne trahis point son esperance. }
    \line { Il n'est point pour les Dieux de mespris innocent, }
    \line { Ils sont jaloux des Cœurs, ils ayment la vengeance, }
    \sline { Il est dangereux qu'on offence }
    \sline { Un amour tout-puissant. }
  }
  \livretPersRef #'DDHchoeur \line { CHŒUR DE SONGES FUNESTES. }
  \livretVers {
    \sline { L'amour qu'on outrage }
    \sline { Se transforme en rage, }
    \sline { Et ne pardonne pas }
    \sline { Aux plus charmants appas. }
    \sline { Si tu n'aymes point Cybele }
    \sline { D'une amour fidelle, }
    \sline { Malheureux, que tu souffriras ! }
    \sline { Tu periras : }
    \sline { Crains une vengeance cruelle, }
    \sline { Tremble, crains un affreux trépas. }
  }
  \livretDesc \justify {
    Atys espouvanté par les Songes funestes, se
    resveille en sursaut, le Sommeil & les Songes
    disparoissent avec l'Antre où ils estoient, & Atys
    se retrouve dans le mesme Palais où il s'estoit
    endormy.
  }
  %%
  \livretScene \line { SCENE CINQUIESME. }
  \livretDesc \wordwrap-center { ATYS, CYBELE, & MELISSE. }
  \livretPersRef #'DEAcybeleAtys ATYS.
  \livretVers {
    \line { Venez à mon secours ô Dieux ! ô justes Dieux ! }
  }
  \livretPers CYBELE.
  \livretVers {
    \line { Atys, ne craignez rien, Cybele, est en ces lieux. }
  }
  \livretPers ATYS.
  \livretVers {
    \line { Pardonnez au desordre où mon cœur s'abandonne ; }
    \line { C'est un songe... }
  }
  \livretPers CYBELE.
  \livretVers {
    \line { \invisible { C'est un songe... } Parlez, quel songe vous estonne ? }
    \sline { Expliquez moy vostre embaras. }
  }
  \livretPers ATYS.
  \livretVers {
    \line { Les songes sont trompeurs, et je ne les croy pas. }
    \sline { Les plaisirs et les peines }
    \sline { Dont en dormant on est séduit, }
    \sline { Sont des chimeres vaines }
    \sline { Que le resveil détruit. }
  }
  \livretPers CYBELE.
  \livretVers {
    \sline { Ne mesprisez pas tant les songes }
    \sline { L'Amour peut souvent emprunter leur voix, }
    \sline { S'ils font souvent des mensonges }
    \sline { Ils disent vray quelquefois. }
    \line { Ils parloient par mon ordre, et vous les devez croire. }
  }
  \livretPers ATYS.
  \livretVers {
    \line { O Ciel ? }
  }
  \livretPers CYBELE.
  \livretVers {
    \line { \invisible { O Ciel ? } N'en doutez point, connoissez vostre gloire. }
    \sline { Respondez avec liberté, }
    \line { Je vous demãde un cœur qui despend de luy-mesme. }
  }
  \livretPers ATYS.
  \livretVers {
    \sline { Une grande Divinité }
    \line { Doit d'assûrer toûjours de mon respect extresme. }
  }
  \livretPers CYBELE.
  \livretVers {
    \sline { Les Dieux dans leur grandeur supresme }
    \line { Reçoivent tant d'honneurs qu'ils en sont rebutez, }
    \line { Ils se lassent souvent d'estre trop respectez, }
    \sline { Ils sont plus contents qu'on les aymes. }
  }
  \livretPers ATYS.
  \livretVers {
    \sline { Je sçay trop ce que je vous doy }
    \sline { Pour manquer de reconnoissance... }
  }
  %%
  \livretScene \line { SCENE SIXIESME. }
  \livretDesc \wordwrap-center { SANGARIDE, CYBELE, ATYS, MELISSE. }
  \livretPersRef #'DFAcybeleAtysSangaride
  \line { SANGARIDE se jettant aux pieds de Cybele. }
  \livretVers {
    \sline { J'ay recours à vostre puissance, }
    \sline { Reyne des Dieux, protegez-moy. }
    \sline { L'interest d'Atys vous en presse... }
  }
  \livretPers \line { ATYS interrompant Sangaride. }
  \livretVers {
    \line { Je parleray pour vous, que vostre crainte cesse. }
  }
  \livretPers SANGARIDE.
  \livretVers {
    \sline { Tous deux unis des plus beaux nœuds... }
  }
  \livretPers \line { ATYS interrompant Sangaride. }
  \livretVers {
    \line { Le sang et l'amitié nous unissent tous deux. }
    \sline { Que vostre secours la délivre }
    \sline { Des loix d'un Hymen rigoureux, }
    \sline { Ce sont les plus doux de ses vœux }
    \line { De pouvoir à jamais vous servir et vous suivre. }
  }
  \livretPers CYBELE.
  \livretVers {
    \sline { Les Dieux sont les protecteurs }
    \sline { De la liberté des cœurs. }
    \line { Allez, ne craignez point le Roy ny sa colere, }
    \sline { J'auray soin d'appaiser }
    \sline { Le Fleuve Sangar vostre Pere ; }
    \sline { Atys veut vous favoriser, }
    \line { Cybele en sa faveur ne peut rien refuser. }
  }
  \livretPers ATYS.
  \livretVers {
    \line { Ah ! c'en est trop... }
  }
  \livretPers CYBELE.
  \livretVers {
    \line { \invisible { Ah ! c'en est trop... } Non, non, il n'est pas necessaire }
    \sline { Que vous cachiez vostre bonheur, }
    \sline { Je ne prétens point faire }
    \sline { Un vain mystere }
    \sline { D'un amour qui vous fait honneur. }
    \line { Ce n'est point à Cybelle à craindre d'en trop dire. }
    \line { Il est vray, j'ayme Atys, pour luy j'ay tout quitté, }
    \line { Sans luy je ne veux plus de grandeur ny d'Empire, }
    \sline { Pour ma felicité }
    \sline { Son cœur seul peut suffire. }
    \line { Allez, Atys luy-mesme ira vous garentir }
    \sline { De la fatale violence }
    \sline { Où vous ne pouvez consentir. }
  }
  \livretDidas \line { Sangaride se retire. }
  \livretPers \line { CYBELE parle à Atys. }
  \livretVers {
    \line { Laissez-nous, attendez mes ordres pour partir, }
    \line { Je prétens vous armer de ma toute-puissance. }
  }
  %%
  \livretScene \line { SCENE SEPTIESME. }
  \livretDesc \wordwrap-center { CYBELE, MELISSE. }
  \livretPersRef #'DGAcybeleMelisse CYBELE.
  \livretVers {
    \line { Qu'Atys dans ses respects mesle d'indifference ! }
    \sline { L'Ingrat Atys ne m'ayme pas ; }
    \line { L'Amour veut de l'amour, tout autre prix l'offence, }
    \line { Et souvent le respect et la reconnoissance }
    \sline { Sont l'excuse des cœurs ingrats. }
  }
  \livretPers MELISSE.
  \livretVers {
    \sline { Ce n'est pas un si grand crime }
    \sline { De ne s'exprimer pas bien, }
    \sline { Un cœur qui n'ayma jamais rien }
    \sline { Sçait peu comment l'amour s'exprime. }
  }
  \livretPers CYBELE.
  \livretVers {
    \line { Sangaride est aymable, Atys peut tout charmer, }
    \sline { Ils tesmoignent trop s'estimer, }
    \line { Et de simples parents sont moins d'intelligence : }
    \sline { Ils se sont aymez dès l'enfance, }
    \sline { Ils pourroient enfin trop s'aymer. }
    \line { Je crains une amitié que tant d'ardeur anime. }
    \sline { Rien n'est si trompeur que l'estime : }
    \sline { C'est un nom supposé }
    \line { Qu'on donne quelquefois à l'amour desguisé. }
    \line { Je prétens m'esclaircir leur feinte sera vaine. }
  }
  \livretPers MELISSE.
  \livretVers {
    \line { Quels secrets par les Dieux ne sont point penetrez ? }
    \sline { Deux cœurs à feindre preparez }
    \sline { Ont beau cacher leur chaîne, }
    \sline { On abuse avec peine }
    \sline { Les Dieux par l'Amour esclairez. }
  }
  \livretPers CYBELE.
  \livretVers {
    \line { Va, Melisse, donne ordre à l'aymable Zephire }
    \line { D'accomplir promptement tout ce qu'Atys desire. }
  }
  %%
  \livretScene \line { SCENE HUITIESME. }
  \livretPersRef #'DHAcybele \line { CYBELE seule. }
  \livretVers {
    \sline { Espoir si cher, et si doux, }
    \sline { Ah ! pourquoy me trompez-vous ? }
    \line { Des suprêmes grãdeurs vous m'avez fait descendre, }
    \line { Mille Cœurs m'adoroient, je les neglige tous, }
    \line { Je n'en demande qu'un, il a peine à se rendre ; }
    \line { Je ne sens que chagrin, et que soupçons jaloux ; }
    \line { Est-ce le sort charmant que je devois attendre ? }
    \sline { Espoir si cher, et si doux, }
    \sline { Ah ! pourquoy me trompez-vous ? }
    \line { Helas ! par tant d'attraits falloit-il me surprendre ? }
    \line { Heureuse, si toûjours j'avois pû m'en deffendre ! }
    \line { L'Amour qui me flattoit me cachoit son couroux : }
    \line { C'est donc pour me fraper des plus funestes coups, }
    \line { Que le cruel Amour m'a fait un cœur si tendre ? }
    \sline { Espoir si cher, et si doux, }
    \sline { Ah ! pourquoy me trompez-vous ? }
  }
  \livretFinAct \line { Fin du troisiéme Acte. }
}
