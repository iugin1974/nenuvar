.PHONY: Rameau/Opera/Paladins-parties
$(OUTPUT_DIR)/Paladins-parties.pdf:
	$(LILYPOND_CMD) \
	-o $(OUTPUT_DIR)/Paladins-parties -dpart=parties  \
	Rameau/Opera/Paladins/part.ly
# Basses
Rameau/Opera/Paladins-basse: $(OUTPUT_DIR)/Paladins-basse.pdf
.PHONY: Rameau/Opera/Paladins-basse
$(OUTPUT_DIR)/Paladins-basse.pdf:
	$(LILYPOND_CMD) \
	-o $(OUTPUT_DIR)/Paladins-basse -dpart=basse  \
	Rameau/Opera/Paladins/part.ly
# Parties vocales
Rameau/Opera/Paladins-voix: $(OUTPUT_DIR)/Paladins-voix.pdf
.PHONY: Rameau/Opera/Paladins-voix
$(OUTPUT_DIR)/Paladins-voix.pdf:
	$(LILYPOND_CMD) \
	-o $(OUTPUT_DIR)/Paladins-voix -dpart=voix  \
	Rameau/Opera/Paladins/part.ly

Rameau/Opera/Paladins-delivery:
	@mkdir -p $(DELIVERY_DIR)/Rameau/Paladins
	@if [ -e $(OUTPUT_DIR)/Paladins.pdf ]; then mv -fv $(OUTPUT_DIR)/Paladins.pdf $(DELIVERY_DIR)/Rameau/Paladins; fi
	@if [ -e $(OUTPUT_DIR)/Paladins-rehearsal.pdf ]; then mv -fv $(OUTPUT_DIR)/Paladins-rehearsal.pdf $(DELIVERY_DIR)/Rameau/Paladins; fi
	@if [ -e $(OUTPUT_DIR)/Paladins-flute1.pdf ]; then mv -fv $(OUTPUT_DIR)/Paladins-flute1.pdf $(DELIVERY_DIR)/Rameau/Paladins; fi
	@if [ -e $(OUTPUT_DIR)/Paladins-flute2.pdf ]; then mv -fv $(OUTPUT_DIR)/Paladins-flute2.pdf $(DELIVERY_DIR)/Rameau/Paladins; fi
	@if [ -e $(OUTPUT_DIR)/Paladins-hautbois1.pdf ]; then mv -fv $(OUTPUT_DIR)/Paladins-hautbois1.pdf $(DELIVERY_DIR)/Rameau/Paladins; fi
	@if [ -e $(OUTPUT_DIR)/Paladins-hautbois2.pdf ]; then mv -fv $(OUTPUT_DIR)/Paladins-hautbois2.pdf $(DELIVERY_DIR)/Rameau/Paladins; fi
	@if [ -e $(OUTPUT_DIR)/Paladins-basson1.pdf ]; then mv -fv $(OUTPUT_DIR)/Paladins-basson1.pdf $(DELIVERY_DIR)/Rameau/Paladins; fi
	@if [ -e $(OUTPUT_DIR)/Paladins-basson2.pdf ]; then mv -fv $(OUTPUT_DIR)/Paladins-basson2.pdf $(DELIVERY_DIR)/Rameau/Paladins; fi
	@if [ -e $(OUTPUT_DIR)/Paladins-violon1.pdf ]; then mv -fv $(OUTPUT_DIR)/Paladins-violon1.pdf $(DELIVERY_DIR)/Rameau/Paladins; fi
	@if [ -e $(OUTPUT_DIR)/Paladins-violon2.pdf ]; then mv -fv $(OUTPUT_DIR)/Paladins-violon2.pdf $(DELIVERY_DIR)/Rameau/Paladins; fi
	@if [ -e $(OUTPUT_DIR)/Paladins-parties.pdf ]; then mv -fv $(OUTPUT_DIR)/Paladins-parties.pdf $(DELIVERY_DIR)/Rameau/Paladins; fi
	@if [ -e $(OUTPUT_DIR)/Paladins-basse.pdf ]; then mv -fv $(OUTPUT_DIR)/Paladins-basse.pdf $(DELIVERY_DIR)/Rameau/Paladins; fi
	@if [ -e $(OUTPUT_DIR)/Paladins-voix.pdf ]; then mv -fv $(OUTPUT_DIR)/Paladins-voix.pdf $(DELIVERY_DIR)/Rameau/Paladins; fi
	@if [ -e $(OUTPUT_DIR)/Paladins.midi ]; then tar zcf $(DELIVERY_DIR)/Rameau/Paladins/Paladins-midi.tar.gz $(OUTPUT_DIR)/Paladins.midi $(OUTPUT_DIR)/Paladins-[0-9]*.midi; fi
	git archive --prefix=Paladins/ HEAD Rameau/Opera/Paladins common out templates Makefile README | gzip > $(DELIVERY_DIR)/Rameau/Paladins/Paladins.tar.gz

Rameau/Opera/Paladins-clean:
	@rm -f $(OUTPUT_DIR)/Paladins-* $(OUTPUT_DIR)/Paladins.*

Rameau/Opera/Paladins-all: \
	$(OUTPUT_DIR)/Paladins.pdf \
	$(OUTPUT_DIR)/Paladins-rehearsal.pdf \
	$(OUTPUT_DIR)/Paladins-flute1.pdf \
	$(OUTPUT_DIR)/Paladins-flute2.pdf \
	$(OUTPUT_DIR)/Paladins-hautbois1.pdf \
	$(OUTPUT_DIR)/Paladins-hautbois2.pdf \
	$(OUTPUT_DIR)/Paladins-basson1.pdf \
	$(OUTPUT_DIR)/Paladins-basson2.pdf \
	$(OUTPUT_DIR)/Paladins-violon1.pdf \
	$(OUTPUT_DIR)/Paladins-violon2.pdf \
	$(OUTPUT_DIR)/Paladins-parties.pdf \
	$(OUTPUT_DIR)/Paladins-basse.pdf \
	$(OUTPUT_DIR)/Paladins-voix.pdf\
	Rameau/Opera/Paladins-delivery\
	Rameau/Opera/Paladins-clean

.PHONY: Rameau/Opera/Paladins-delivery Rameau/Opera/Paladins-clean Rameau/Opera/Paladins-all

