  \new Staff = "down" <<
    \global \clef "bass"
    \new Voice = "one" {
      \set tieWaitForNote = ##t
      %%
      %% Premier rondeau
      <la, la,,>2 |
      q q |
      q <la, mi sol!> |
      << { fa8 mi re dod } \\ <la la,>2 >> <re re,> |
      <mi, mi>8 re do\prall si, <la, la,,>2 |
      q q |
      q <la, mi sol!> |
      << { fa8 <mi sol> s8 r r4 mi } \\
        { <la la,>4.*2/3 \once \override Beam #'positions = #'(-2 . -1)
          \stemDown <re fa>8 \stemUp re, \stemDown mi,2 } >> |
      << \para la,2 \\ { r4 la,, }>> <la, la,,>2 |
      %% première reprise
      la,8 mi fad sold \para la4 sol8\prall fa, |
      \slurDown mi( sol \para do'~ mi)
      << { \voiceOne do'8 do'4 si8 } \new Voice { \voiceTwo fa4 sol } >> |
      \oneVoice do8( do') mi( do')~
      << { \voiceOne do'8 do' re'4 } \new Voice { \voiceTwo fa4. fa8 } >> |
      \oneVoice \slurDown mi( sol \para do'~ mi)
      << { \voiceOne do'8 do'4 si8 } \new Voice { \voiceTwo fa4 sol } >> |
      \oneVoice do8( do') mi( do')~
      << { \voiceOne do'8 do' re'4 } \new Voice { \voiceTwo fa4. fa8 } >> |
      << { r8 sol \para do'4~ do'4. } \\ { mi2~ mi8 fa fa <mi sol> }>> |
      \oneVoice <re fa>4. <do sol>8 <si, sol>4. <la la,>8 |
      <sol, sol>4 <fa fa,> <mi, mi>2 |
      <fa fa,>8 <sol, sol> <la la,> <fa, fa> <sol sol,~>4 <sol, sol,,> |
      <do, do>8 re do\prall si,
      %% rondeau
      <la, la,,>2 |
      q q |
      q <la, mi sol!> |
      << { fa8 mi re dod } \\ <la la,>2 >> <re re,> |
      <mi, mi>8 re do\prall si, <la, la,,>2 |
      q q |
      q <la, mi sol!> |
      << { fa8 <mi sol> s8 r r4 mi } \\
        { <la la,>4.*2/3 \once \override Beam #'positions = #'(-2 . -1)
          \stemDown <re fa>8 \stemUp re, \stemDown mi,2 } >> |
      %% deuxième reprise
      la,8 mi la si do' sold la red |
      mi si mi' re' do'( mi') si( re') |
      do' mi' la do' << { sol mi' si sol } \\ sol4 >> |
      fad8( la red fad) << { r8 si dod'~ <dod' mi'> } \\ mi2 >> |
      <si red'>8\prall la sol\prall fad << sol \\ { sol4 la }>> |
      sol4. sol8 fad4. si8 |
      mi4 si << { sol8 } \\ { sol4 la } >> |
      sol4. sol8 fad4. si8 |
      <mi mi'>4 <si red'> <sol si mi'>2 |
      <fad la si red'>4 <mi si mi'> <si, si>2 |
      <mi mi'>8 re' do'\prall si
      %% rondeau
      <la, la,,>2 |
      q q |
      q <la, mi sol!> |
      << { fa8 mi re dod } \\ <la la,>2 >> <re re,> |
      <mi, mi>8 re do\prall si, <la, la,,>2 |
      q q |
      q <la, mi sol!> |
      << { fa8 <mi sol> s8 r r4 mi } \\
        { <la la,>4.*2/3 \once \override Beam #'positions = #'(-2 . -1)
          \stemDown <re fa>8 \stemUp re, \stemDown mi,2 } >> |
      << \para la,2 \\ { r4 la,, }>>
      %%
      %% Deuxième rondeau
      r2 |
      la8 si dod' la si4 mi'8 mi |
      fad fad' mi'\prall re' dod' re' mi' dod' |
      re' dod' si la sold4.\prall la8 |
      mi8 re' dod'\prall si la2 |
      la,8 si, dod la, si,4 mi8 mi, |
      fad, fad mi\prall re dod re mi dod |
      re dod si,\prall la, << mi2 \\ { r4 mi, }>> |
      << la,2 \\ { r4 la,, }>> r2 |
      %% première reprise
      la,8 mi la dod' la2\prall~ |
      la8 la dod' mi' dod'2\prall |
      s2 <la la,> |
      q r4 r8 <la mi'>8 |
      <sold mi'>4. <sold si mi'>8 << { <dod' mi'>8[ <si red'>] } \\ fad4 >> <mi si mi'>4 |
      si4. <si si,>8 <mi mi,>4. q8 |
      <si,, si,>4. q8 <mi mi,>4. sold8 |
      \parc la4 la8.\prall sold32 la si4 <la la,> |
      <mi, mi>8 re dod\prall si,
      %% rondeau
      la,2 |
      la8 si dod' la si4 mi'8 mi |
      fad fad' mi'\prall re' dod' re' mi' dod' |
      re' dod' si la sold4.\prall la8 |
      mi8 re' dod'\prall si la2 |
      la,8 si, dod la, si,4 mi8 mi, |
      fad, fad mi\prall re dod re mi dod |
      re dod si,\prall la, << mi2 \\ { r4 mi, }>> |
      %% deuxième reprise
      << { \para la,2 } \\ { r4 la,, }>> \downup dod''8 si' la'\prall sold' |
      la'16 dod'' la' fad' s8 \downdown \voiceTwo fad16 fad, \downup dod''8 si' la'\prall sold' |
      la'16 dod'' la' fad' s8 \downdown \voiceTwo fad16 fad, \oneVoice s2 |
      <si, si,,>4 \downup <si fad' si'> r16 si' sold' mi' s8 \downdown \voiceTwo mi16 mi, |
      <la, la,,>4 \downup <la mi' la'> r16 la' fad' re' s8 \downdown \voiceTwo re16 re, |
      <sold, sold,,>4 \downup <sold sold'>4 r16 sold' mid' dod' s8 \downdown \voiceTwo dod16~ dod,~ |
      <dod, dod>2 \oneVoice << { <sold si>4. <fad la>8 } \\ dod2 >> |
      s1*2 |
      s2. <la, dod fad>4 |
      <si, re fad>2 << dod2 \\ { r4 dod, } >> |
      <fad, fad>8 la, sold, si,
      %% rondeau
      la,2 |
      la8 si dod' la si4 mi'8 mi |
      fad fad' mi'\prall re' dod' re' mi' dod' |
      re' dod' si la sold4.\prall la8 |
      mi8 re' dod'\prall si la2 |
      la,8 si, dod la, si,4 mi8 mi, |
      fad, fad mi\prall re dod re mi dod |
      re dod si,\prall la, << mi2 \\ { r4 mi, }>> |
      << { \para la,2 } \\ { r4 la,, }>>
    }
